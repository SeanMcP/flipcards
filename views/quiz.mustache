<h1 id="quizDirections">Quiz yourself and see how well you know the information</h1>
{{#data}}
  {{#.}}
    <article class="quizItem">
      <div class="cardContainer">
        <section class="quizFront quizCard">
          {{front}}
        </section>
        <section class="quizBack quizCard">
          {{back}}
        </section>
      </div>
      <aside class="quizScore">
        <a href="#" class="scoreButton plus align-center">Got it!<i class="material-icons">thumb_up</i></a>
        <a href="#" class="scoreButton minus align-center"><i class="material-icons">thumb_down</i>Not yet</a>
      </aside>
    </article>
  {{/.}}
{{/data}}
<script type="text/javascript" src="/js/main.js"></script>
